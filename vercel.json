{
    "version": 2,
    "build": {
        "env": {
            "PIP_BREAK_SYSTEM_PACKAGES": "1"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/$1"
        }
    ]
}